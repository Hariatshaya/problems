public class PaintingCost {
    public static void main(String[] args) {
        int intWalls = 6;
        int extWalls = 3;

        double[] intAreas = {12.3, 15.2, 12.3, 15.2, 12.3, 15.2};
        double[] extAreas = {10.10, 10.10, 10.00};

        double total = 0;

        for (double area : intAreas) total += area * 18;
        for (double area : extAreas) total += area * 12;

        System.out.printf("Total estimated Cost : %.1f INR\n", total);
    }
}
